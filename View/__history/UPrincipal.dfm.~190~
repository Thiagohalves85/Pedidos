    SQL.Strings = (
      'SELECT I.ID, I.QUANTIDADE, I.TOTALITEM,'
      '       PR.DESCRICAO, PR.VALOR'
      'FROM ITEMPEDIDO I'
      'LEFT JOIN PRODUTO PR ON I.IDPRODUTO = PR.ID '
      'WHERE I.IDPEDIDO = :ID')

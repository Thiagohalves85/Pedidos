  object qryNota: TUniQuery
    Connection = con1
    SQL.Strings = (
      'SELECT '
      '  N.ID,N.IDPEDIDO, N.VALOR, '
      ' CASE N.TIPO'
      '  WHEN '#39'E'#39' THEN '#39'ENTRADA'#39
      '  WHEN '#39'S'#39' THEN '#39'SAIDA'#39' '
      '  END AS TIPO, '
      ' CASE N.TIPO'
      '  WHEN '#39'E'#39' THEN '#39'Compra'#39
      '  WHEN '#39'S'#39' THEN '#39'Venda'#39
      '  END AS NATUREZA, '
      ' CASE N.PAGAMENTO'
      ' WHEN '#39'A'#39' THEN '#39'A VISTA'#39
      ' WHEN '#39'P'#39' THEN '#39'A PRAZO'#39
      ' END AS PAGAMENTO, '
      ' N.DATAEMISSAO,'
      'P.NOMECLIENTE,'
      'P.CPFCLIENTE,'
      'P.EMAILCLIENTE'
      'FROM NOTA N '
      'LEFT JOIN PEDIDO P ON P.ID = N.IDPEDIDO'
      'WHERE P.ID = :ID')
    Left = 749
    Top = 154
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'ID'
        Value = nil
      end>

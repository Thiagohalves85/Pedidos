  object qryGridItens: TUniQuery
    Connection = con1
    SQL.Strings = (
      'SELECT I.ID as NUMPEDIDO,  '
      '       PR.ID, PR.DESCRICAO, '
      '       I.QUANTIDADE, I.TOTALITEM'
      'FROM ITEMPEDIDO I'
      'LEFT JOIN PEDIDO P ON P.ID = I.IDPEDIDO'
      'LEFT JOIN PRODUTO PR ON PR.ID = I.IDPRODUTO '
      'WHERE P.ID = :ID')
    Left = 529
    Top = 73
    ParamData = <
      item
        DataType = ftInteger
        Name = 'ID'
        ParamType = ptInput
